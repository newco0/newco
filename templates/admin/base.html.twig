		<meta http-equiv="X-UA-Compatible" content="ie=edge">
		<title>
			{% block title %}Panel Administrateur
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.4.1/css/bootstrap.min.css">
